{
  "AllowedHosts": "*",
  "AuthSettings": {
    "Audience": "RefreshToken.API",
    "Issuer": "https://refreshtoken.test",
    "RefreshToken": {
      "ExpiresInSeconds": 120
    },
    "Token": {
      "ExpiresInSeconds": 120
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "SwaggerSettings": {
    "Description": "Exemplo de implementação de refresh token.",
    "Title": "RefreshToken API",
    "Version": "v1"
  }
}
